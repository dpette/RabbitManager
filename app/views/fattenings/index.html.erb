<p id="notice"><%= notice %></p>

<h1>Listing Fattenings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fattenings.each do |fattening| %>
      <tr>
        <td><%= link_to 'Show', fattening %></td>
        <td><%= link_to 'Edit', edit_fattening_path(fattening) %></td>
        <td><%= link_to 'Destroy', fattening, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fattening', new_fattening_path %>
